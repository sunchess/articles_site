<li>
  <%= link_to image_tag( image.pic.url(:thumb) ), image.pic.url(:biggest) %> <%= check_box_tag "hidden[]", image.id %>
  <br/>
  <% Image::VARIANTS.each do |key, val| %>
    <%= val %><br/> <%= text_field_tag key, "http://#{request.env["HTTP_HOST"]}#{ image.pic.url(key).gsub(/\?\d+/, "") }", :onfocus=>"this.select();" %><br/>
  <% end %>
</li>
